# Server Configuration
PORT=3000
NODE_ENV=development

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=secret (Optional)

# WebAuthn Configuration (Required for Passkeys)
# In production, WEBAUTHN_ORIGIN must be HTTPS
WEBAUTHN_RP_NAME=SecureAuth-2FA
WEBAUTHN_RP_ID=localhost
WEBAUTHN_ORIGIN=http://localhost:3000

# Security Tuning (Optional)
# RATE_LIMIT_WINDOW=300000 (5 minutes in ms)
# RATE_LIMIT_MAX=5
